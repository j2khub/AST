# 키움 API 인증 정보
# https://openapi.kiwoom.com/ 에서 발급받은 정보를 입력하세요

# 앱 키 (App Key)
APPKEY=your_app_key_here

# 앱 시크릿 키 (주의: secretkey 사용, appsecret 아님)
SECRETKEY=your_secret_key_here

# 접근 토큰 (주의: token 사용, access_token 아님)
# au10001 API로 발급받은 토큰을 입력하거나, 자동 발급 설정 시 비워두세요
TOKEN=

# 계좌 정보
ACCOUNT_NO=your_account_number_here

# API 설정
API_BASE_URL=https://openapi.kiwoom.com:9443

# 운영 모드 (development / production)
MODE=development

# 로깅 레벨 (DEBUG / INFO / WARNING / ERROR)
LOG_LEVEL=INFO

# 자동매매 설정
AUTO_TRADE_ENABLED=false
MAX_BUY_AMOUNT=1000000
MAX_POSITION_COUNT=5
